pipeline {
    agent any 
    stages {
        stage('SCM Checkout') {
            steps {
                echo 'Chekcing out source code'
            }
        }
        stage ('compile and package') {
            steps {
                def MAVEN_HOME = tool name: 'maven-1',type:'maven'
                sh "${MAVEN_HOME}/bin/mvn clean package"
            }
        }
    }
}
